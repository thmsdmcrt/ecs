{
	"parser": "@babel/eslint-parser",
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "2021",
        "sourceType": "module",
        "requireConfigFile": false
    },
    "rules": {
    	"indent": ["error", "tab"],
    	"no-tabs": "off"
    }
}